<h2>Comments</h2>

<ul>
 {% for comment in comments %}
        <li>{{ comment.name|e }}</li>
        <ul>
            <li>{{ comment.id }}</li>
            <li>id: {{ comment.code }}</li>
            <li>user id: {{ comment.userId }}</li>
            <li>Comment: {{ comment.comment|e }}</li>
            <li>Date: {{ comment.createdAt|date }}</li>
            <li>IP: {{ comment.Ip }}</li>
        </ul>
    {% endfor %}
</ul>